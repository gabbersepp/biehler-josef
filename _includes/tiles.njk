{% if 'project' in page.url %}
    {% set class = 'project-tile' %}
{% endif %}
{% if 'dev-blog' in page.url %}
    {% set class = 'blog-tile' %}
{% endif %}

<div class="tiles-header"></div>

<div id="tiles">
    {% for entry in data %}
        <div class="tile-wrapper">
            <div class="tile {{class}}">
                {% include tileFile %}
            </div>
        </div>
    {% endfor %}
</div>